{
  "manifest_version": 2,
  "name": "JD Express Ads Remover",
  "version": "1.0",
  "description": "Removes JD Express opening screen ads",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.jd.com/*"],
      "js": ["background.js"]
    }
  ]
}